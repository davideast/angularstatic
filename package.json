{
  "name": "angularstatic",
  "version": "0.0.3",
  "description": "A simple way to build static templates with Angular",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && mocha dist/packages-dist/index.spec.js",
    "build": "rm -rf dist && tsc && cp src/package.json dist/packages-dist && cp README.md dist/packages-dist",
    "pack": "npm pack dist/packages-dist"
  },
  "keywords": [
    "angular"
  ],
  "author": "David East",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.4.4",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.2",
    "ts-node": "^3.2.0",
    "typescript": "^2.5.3",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@types/jasmine": "^2.6.0",
    "@types/jsdom": "^11.0.2",
    "jasmine": "^2.8.0",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.1"
  },
  "repository": "https://github.com/davideast/angularstatic.git"
}
